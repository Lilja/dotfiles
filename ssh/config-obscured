
Host hoppa-danksand-net
    Hostname hoppa.danksand.net
    Port 2307
    User jump


Host yoda-remote
    Hostname yoda.lan
    User root
    PreferredAuthentications password
    PubkeyAuthentication no
    ProxyJump hoppa-danksand-net

Host yoda-remote-gui
    Hostname yoda.lan
    User root
    PreferredAuthentications password
    PubkeyAuthentication no
    LocalForward 8006 localhost:8006
    ProxyJump hoppa-danksand-net
    LocalCommand /bin/bash open https://localhost:8006
    RequestTTY no
